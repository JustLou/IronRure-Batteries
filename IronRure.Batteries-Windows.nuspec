    <file src="contents\runtimes\win-x64\native\rure.dll" target="contentFiles\any\net45\rure_x64\rure.dll" />
    <file src="contents\runtimes\win-x86\native\rure.dll" target="contentFiles\any\net45\rure_x86\rure.dll" />
  </files>
</package>
